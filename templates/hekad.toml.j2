[hekad]
max_message_loops = {{ heka_hekad_max_message_loops }}
max_process_inject = {{ heka_hekad_max_process_inject }}
max_process_duration = {{ heka_hekad_max_process_duration }}
max_timer_inject = {{ heka_hekad_max_timer_inject }}
maxprocs = {{ heka_hekad_maxprocs }}
poolsize = {{ heka_hekad_poolsize }}
plugin_chansize = {{ heka_hekad_plugin_chansize }}
sample_denominator = {{ heka_hekad_sample_denominator }}
base_dir = "{{ heka_hekad_base_dir }}"
share_dir = "{{ heka_hekad_share_dir }}"
{% if heka_hekad_cpuprof %}
cpuprof = true
{% endif %}
{% if heka_hekad_memprof %}
memprof = true
{% endif %}
pid_file = "{{ heka_hekad_pidfile }}"
